    "lint:jsfix": "eslint --fix --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint:stylefix": "stylelint --fix \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint:jsfix && npm run lint:stylefix",
    "prettier:fix": "npx prettier --write .",
    "check": "echo 'npm OK'",
    "release:major": "npm version major -m 'upgrade to major version %s'",
    "release:minor": "npm version minor -m 'upgrade to minor version %s'",
    "release:patch": "npm version patch -m 'upgrade to patch version %s'",
    "postversion": "git checkout master && git push && git push --tags"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
